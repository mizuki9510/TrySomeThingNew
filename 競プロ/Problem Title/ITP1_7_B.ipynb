    "while True:\n",
    "    cnt = 0\n",
    "    N,X = map(int,input().split())\n",
    "    if N == X == 0:\n",
    "        break\n",
    "        \n",
    "    for i in range(1,N):\n",
    "        for j in range(i+1,N+1):\n",
    "            for k in range(j+1,N+1):\n",
    "                if i + j + k == X :\n",
    "                    cnt = cnt + 1\n",
    "    print(cnt)"
   ]
  }
